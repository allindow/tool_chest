<h1>Admin Categories<h2>
<ul id="categories_list">
  <% @categories.each do |category| %>
    <li><%= category.name %>
      <p><%= link_to "Edit", edit_admin_category_path(category) %>
          <%= link_to "Delete", admin_category_path(category), method: :delete %>
      </p></li>
    <% end %>
</ul>
